{
  "language": "en",
  "dictionaries": [
    "dict"
  ],
  "dictionaryDefinitions": [
    {
      "name": "dict",
      "path": "./dict.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    // binaries
    "**/*.a",
    "**/*.dll",
    "**/*.dylib",
    "**/*.jar",
    "**/*.mp3",
    "**/*.node",
    "**/*.ppn",
    "**/*.pv",
    "**/*.so",
    "**/*.wasm",
    "**/*.wav",
    "**/*.egl",
    "../../../lib/ios/**/*",

    // ios
    "**/*.xcconfig",
    "**/*.podspec",
    "**/Podfile",
    "**/*.plist",
    "**/contents.xcworkspacedata",
    "**/*.storyboard",
    "**/*.pbxproj",
    "**/*.xcscheme",
    "**/*.m",
    
    // android & java
    "**/AndroidManifest.xml",
    "**/activity_main.xml",

    // java
    "**/build.gradle",
    "**/gradle-wrapper.properties",
    "**/gradle.properties",
    "**/gradlew",
    "**/gradlew.bat",
    "**/proguard-rules.pro"
  ]
}
